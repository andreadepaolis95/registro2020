<?xml version="1.0" encoding="UTF-8"?>
<project name="registro2020" default="all">
  <target name="build" />
  
  <property file="build.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*.yarb/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.egg"/>
    <include name="*.jar"/>
    <include name="*.ear"/>
    <include name="*.klib"/>
    <include name="*.swc"/>
    <include name="*.war"/>
    <include name="*.zip"/>
    <include name="*.ane"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
    <exclude name="**/?*.aj"/>
  </patternset>
  
  
  <!-- Project Libraries -->
  
  <path id="library.jstl-1.2.classpath">
    <pathelement location="${basedir}/out/artifacts/registro2_war_exploded/WEB-INF/lib/jstl-1.2.jar"/>
  </path>
  
  
  <!-- Global Libraries -->
  
  <path id="library.javafx-sdk-13.0.classpath">
    <fileset dir="/Users/andrea/Downloads/javafx-sdk-13.0.1">
      <patternset refid="library.patterns"/>
    </fileset>
  </path>
  
  
  <!-- Application Server Libraries -->
  
  <path id="library.tomcat_9.0.27.classpath">
    <pathelement location="/Users/andrea/eclipse/apache-tomcat-9.0.27/lib/jsp-api.jar"/>
    <pathelement location="/Users/andrea/eclipse/apache-tomcat-9.0.27/lib/servlet-api.jar"/>
  </path>
  
  <!-- Modules -->
  
  
  <!-- Module registro2020 -->
  
  <dirname property="module.registro2020.basedir" file="${ant.file}"/>
  
  
  
  <property name="compiler.args.registro2020" value="-encoding UTF-8 -source 13 -target 13 ${compiler.args}"/>
  
  <property name="registro2020.output.dir" value="${module.registro2020.basedir}/out/production/registro2020"/>
  <property name="registro2020.testoutput.dir" value="${module.registro2020.basedir}/out/test/registro2020"/>
  
  <path id="registro2020.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="registro2020.module.production.classpath">
    <path refid="library.tomcat_9.0.27.classpath"/>
    <path refid="library.javafx-sdk-13.0.classpath"/>
    <path refid="library.jstl-1.2.classpath"/>
  </path>
  
  <path id="registro2020.runtime.production.module.classpath">
    <pathelement location="${registro2020.output.dir}"/>
    <path refid="library.javafx-sdk-13.0.classpath"/>
    <path refid="library.jstl-1.2.classpath"/>
  </path>
  
  <path id="registro2020.module.classpath">
    <pathelement location="${registro2020.output.dir}"/>
    <path refid="library.tomcat_9.0.27.classpath"/>
    <path refid="library.javafx-sdk-13.0.classpath"/>
    <path refid="library.jstl-1.2.classpath"/>
  </path>
  
  <path id="registro2020.runtime.module.classpath">
    <pathelement location="${registro2020.testoutput.dir}"/>
    <pathelement location="${registro2020.output.dir}"/>
    <path refid="library.tomcat_9.0.27.classpath"/>
    <path refid="library.javafx-sdk-13.0.classpath"/>
    <path refid="library.jstl-1.2.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.registro2020">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.registro2020">
    <patternset refid="excluded.from.module.registro2020"/>
  </patternset>
  
  <path id="registro2020.module.sourcepath">
    <dirset dir="${module.registro2020.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.registro2020" depends="compile.module.registro2020.production,compile.module.registro2020.tests" description="Compile module registro2020"/>
  
  <target name="compile.module.registro2020.production" description="Compile module registro2020; production classes">
    <mkdir dir="${registro2020.output.dir}"/>
    <javac destdir="${registro2020.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true">
      <compilerarg line="${compiler.args.registro2020}"/>
      <bootclasspath refid="registro2020.module.bootclasspath"/>
      <classpath refid="registro2020.module.production.classpath"/>
      <src refid="registro2020.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.registro2020"/>
    </javac>
    
    <copy todir="${registro2020.output.dir}">
      <fileset dir="${module.registro2020.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.registro2020.tests" depends="compile.module.registro2020.production" description="compile module registro2020; test classes" unless="skip.tests"/>
  
  <target name="clean.module.registro2020" description="cleanup module">
    <delete dir="${registro2020.output.dir}"/>
    <delete dir="${registro2020.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.registro2020, clean.artifact.unnamed" description="cleanup all"/>

  <target name="build.modules" depends="init, clean, compile.module.registro2020" description="build all modules"/>

  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.unnamed" value="${basedir}/out/artifacts/unnamed"/>
    <mkdir dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="clean.artifact.unnamed" description="clean unnamed artifact output">
  </target>
  
  <target name="artifact.unnamed" depends="init.artifacts, compile.module.registro2020" description="Build &#39;unnamed&#39; artifact">
    <mkdir dir="${artifact.output.unnamed}"/>
    <mkdir dir="${artifact.output.unnamed}/WEB-INF"/>
    <mkdir dir="${artifact.output.unnamed}/WEB-INF/classes"/>
    <copy todir="${artifact.output.unnamed}/WEB-INF/classes">
      <fileset dir="${registro2020.output.dir}"/>
    </copy>
    <mkdir dir="${artifact.output.unnamed}/WEB-INF/lib"/>
    <copy file="${basedir}/out/artifacts/registro2_war_exploded/WEB-INF/lib/jstl-1.2.jar" tofile="${artifact.output.unnamed}/WEB-INF/lib/jstl-1.2.jar"/>
    <copy todir="${artifact.output.unnamed}">
      <fileset dir="${basedir}/out/artifacts/registro2_war_exploded"/>
    </copy>
    <mkdir dir="${artifact.output.unnamed}/WEB-INF"/>
    <copy file="${basedir}/out/artifacts/registro2_war_exploded/WEB-INF/web.xml" tofile="${artifact.output.unnamed}/WEB-INF/web.xml"/>
    <copy todir="${artifact.output.unnamed}">
      <fileset dir="${basedir}/web"/>
    </copy>
    <mkdir dir="${artifact.output.unnamed}/WEB-INF"/>
    <copy file="${basedir}/web/WEB-INF/web.xml" tofile="${artifact.output.unnamed}/WEB-INF/web.xml"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.unnamed" description="Build all artifacts">
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
</project>